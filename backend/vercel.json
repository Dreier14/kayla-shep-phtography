{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "destination": "/"
        }
    ],
    "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
